{
  "name": "serverless-vtuber-scheduler",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/syu-y/serverless-vtuber-scheduler.git",
  "author": "syu-y <syu12580526@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dynamoDB": "echo ###### DynamoDB Scripts ######",
    "dynamodb:setup": "sls dynamodb install -c ./src/app/dynamodb/serverless.yml",
    "dynamodb:start": "sls dynamodb start -c ./src/app/dynamodb/serverless.yml",
    "dynamodb:deploy": "sls deploy -c ./src/app/dynamodb/serverless.yml",
    "dynamodb:deploy:prod": "sls deploy --stage prod -c ./src/app/dynamodb/serverless.yml",
    "dynamodb:rm": "sls remove -c ./src/app/dynamodb/serverless.yml",
    "dynamodb:rm:prod": "sls remove --stage prod -c ./src/app/dynamodb/serverless.yml",
    "invoke": "echo ###### Invoke Lambda Local ######",
    "invoke:get-youtube-broadcast": "LOCAL=true sls invoke local -f getYoutubeBroadcast -c ./src/app/lambda/serverless.yml",
    "invoke:get-channel-info": "LOCAL=true sls invoke local -f getChannelInfo -c ./src/app/lambda/serverless.yml",
    "invoke:youtube-broadcast-api": "LOCAL=true sls invoke local -f youtubeBroadcastInfoApi -c ./src/app/lambda/serverless.yml",
    "invoke:aws": "echo ###### Invoke Lambda AWS ######",
    "invoke:aws:get-youtube-broadcast": "sls invoke -f getYoutubeBroadcast -c ./src/app/lambda/serverless.yml",
    "invoke:aws:get-channel-info": "sls invoke -f getChannelInfo -c ./src/app/lambda/serverless.yml",
    "invoke:aws:youtube-broadcast-api": "sls invoke -f youtubeBroadcastInfoApi -c ./src/app/lambda/serverless.yml",
    "lambda": "echo ###### Deploy Lambda ######",
    "lambda:deploy:all": "sls deploy -c ./src/app/lambda/serverless.yml",
    "lambda:deploy:get-youtube-broadcast": "sls deploy function -f getYoutubeBroadcast -c ./src/app/lambda/serverless.yml",
    "invoke:deploy:get-channel-info": "sls deploy function -f getChannelInfo -c ./src/app/lambda/serverless.yml",
    "invoke:deploy:youtube-broadcast-api": "sls deploy function -f youtubeBroadcastInfoApi -c ./src/app/lambda/serverless.yml",
    "lambda:deploy:prod": "sls deploy --stage prod -c ./src/app/lambda/serverless.yml",
    "lambda:rm": "sls remove -c ./src/app/lambda/serverless.yml",
    "lambda:rm:prod": "sls remove --stage prod -c ./src/app/lambda/serverless.yml"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.8.28",
    "express": "^4.17.1",
    "serverless-http": "^2.5.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "aws-sdk": "^2.685.0",
    "babel-loader": "^8.1.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.1.4",
    "serverless-webpack": "^5.3.2",
    "webpack": "^4.42.1"
  }
}
